var mes = date.getMonth('mm');
var ndia = date.getDate('dd');
var ano= date.getUTCFullYear();
var hora= date.getHours();
var minutos = date.getMinutes();
var segundos = date.getSeconds();
mes+=1;
if(mes<10) mes="0"+mes;
var fecha = ano + "-" + mes + "-" + ndia  + " " + hora +":"+ minutos +":"+ segundos;
